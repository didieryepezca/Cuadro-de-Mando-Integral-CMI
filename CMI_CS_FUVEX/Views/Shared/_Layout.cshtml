    <!-- TOAST CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css" />


    <script>


        function funActualizarFecha() {            
            

            var vfecha = $('#fecha1').val();
            var FechaString = "";

            $('#FechaConsulta').remove();  

            var url = "/Home/funActualizarFecha";
            $.get(url, { fecha: vfecha }, function (data) {

                console.log(data);

                //$('#fecha1').val(data);  


                FechaString = FechaString + '<h6 class="date" id="FechaConsulta"> ' + vfecha + ' </h5>';
                $('#CardFechaConsulta').append(FechaString);               
                

                toastr.success('Fecha de consulta: ' + vfecha + '', 'CMI Alerta',
                    {
                        "closeButton": true,
                        "debug": false,
                        "newestOnTop": false,
                        "progressBar": false,
                        "positionClass": "toast-bottom-right",
                        "preventDuplicates": false,
                        "onclick": null,
                        "showDuration": "300",
                        "hideDuration": "0",
                        "timeOut": "0",
                        "extendedTimeOut": "1000",
                        "showEasing": "swing",
                        "hideEasing": "linear",
                        "showMethod": "fadeIn",
                        "hideMethod": "fadeOut"
                    })

            });


        };

    </script>


